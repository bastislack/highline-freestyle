{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint --ext .ts,.vue,.json src",
    "lint:eslint:fix": "eslint --ext .ts,.vue,.json src --fix",
    "lint:prettier": "prettier src --check",
    "lint:prettier:fix": "prettier src --write",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "dexie": "^3.2.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.27",
    "vue-router": "^4.2.4",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.15",
    "chalk": "^5.3.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "globby": "^13.2.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.29",
    "prettier": "3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-node": "^0.34.6",
    "vite-plugin-pwa": "^0.16.5",
    "vue-tsc": "^1.8.5",
    "yaml": "^2.3.2",
    "zod-to-json-schema": "^3.21.4",
    "zx": "^7.2.3"
  }
}
